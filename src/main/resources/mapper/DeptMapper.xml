<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zpyu.bootjsp2.dao.DeptMapper">
    <!-- 通过ID查询一个用户 -->
    <select id="find" resultType="com.zpyu.bootjsp2.entity.Dept">
        SELECT login_name as loginName,age as age
        FROM account;
    </select>

</mapper>